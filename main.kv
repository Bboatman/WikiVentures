#:kivy 1.9.1

<Player>:
	size: (self.height, self.width)
	canvas:
		Rectangle:
			pos: (self.x, self.y)
			size: (self.height, self.width)
			source: self.source

<Enemy>:
	size: (self.height, self.width)
	canvas:
		Rectangle:
			pos: (self.x, self.y)
			size: (self.height, self.width)
			source: self.source